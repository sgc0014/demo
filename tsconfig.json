{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl":".",
    "paths":{
      "@files/*":["src/files/*"],
      "@components/*":["src/components/*"],
      "@views/*": ["src/views/*"],
      "@theme/*": ["src/theme/*"],
      "@common/*": ["src/common/*"],
      "@store/*":["src/store/*"],
     
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/pages/dashboard.tsx"],
  "exclude": ["node_modules"]
}
